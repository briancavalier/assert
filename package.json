{
  "name": "@briancavalier/assert",
  "version": "3.4.0",
  "description": "Simple, fast, curried test assertions",
  "author": "brian@hovercraftstudios.com",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:flow",
    "test:unit": "nyc -s mocha -r buba/register",
    "test:lint": "prettier-standard '{src,test}/**/*.js' && standard --verbose '{src,test}/**/*.js' | snazzy",
    "test:flow": "flow check",
    "test:report": "nyc report",
    "ci": "npm run test:lint && npm run test:unit && npm run test:flow && npm run test:report",
    "build": "npm run build:dist && npm run build:flow && npm run build:ts",
    "build:dist": "rollup -c",
    "build:flow": "cpy src/index.js.flow dist",
    "build:ts": "cpy src/index.d.ts dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/briancavalier/assert"
  },
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-preset-flow": "^6.23.0",
    "buba": "^4.0.2",
    "cpy-cli": "^1.0.1",
    "eslint-plugin-flowtype": "^2.37.0",
    "flow-bin": "^0.56.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "prettier-standard": "^7.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "@most/prelude": "^1.5.0",
    "lodash.isequal": "^4.5.0",
    "object-inspect": "^1.2.1"
  },
  "babel": {
    "presets": [
      "flow"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "eslint-plugin-flowtype"
    ]
  }
}
